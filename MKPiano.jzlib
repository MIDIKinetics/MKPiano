<SCRIPT name="touchVelocity(keyboard, key, y)" script="// Calculate dimensions
decl keyboardHeight = getobjectrect(keyboard)[3];
decl blackKeyHeight = keyboardHeight / 2;
decl hitMinY, hitMaxY;

// Constrain the hit area for white and black keys
if (isWhiteKey(key)) {
    hitMinY = blackKeyHeight + 32;
    hitMaxY = keyboardHeight - 32;
} else {
    hitMinY = 16;
    hitMaxY = blackKeyHeight - 32;
}

// Calculate touch location percent of height
decl touchHeight = hitMaxY - hitMinY;
decl touchLocationPercent = min(max(1 - ((y - hitMinY) / touchHeight), 0), 1);

// Apply custom scaling for black keys to make low touches yield higher velocity
decl scaledVelocity;
if (isWhiteKey(key)) {
    scaledVelocity = touchLocationPercent; // Linear for white keys
} else {
    // Power curve for black keys to boost velocity at lower touches
    scaledVelocity = pow(touchLocationPercent, 0.5);  // Adjust exponent as needed
}

// Set higher minimum velocity to improve audibility and return scaled result
return max(round(scaledVelocity * 127), 48);
