decl totalOctaves = 11;

decl i;
for (i = 0; i < sizeof(keyboards); i++) {
	setexpression(keyboards[i], 'lowestOctave', -2 + (i * 2));
	setexpression(keyboards[i], 'numOctaves', 2);
}


